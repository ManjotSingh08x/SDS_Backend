<!DOCTYPE html>
<html>
<head>
    <title>User List</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }
        th, td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1 style="text-align: center;">User List</h1>
    <table style="margin: 0 auto; text-align: center;">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Address</th>
                <th>Country</th>
            </tr>
        </thead>
        <tbody>
            <!-- Replace the following rows with your user data -->
            <% users.forEach(function(user) { %>
            <tr>
                <td><%= user.user_id %></td>
                <td><%= user.user_name %></td>
                <td><%= user.user_role %></td>
                <td><%= user.password %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>